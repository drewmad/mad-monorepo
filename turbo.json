{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "@db#build": {
      "outputs": [
        "dist/**"
      ]
    },
    "@apps/web#build": {
      "dependsOn": [
        "@db#build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    }
  }
}